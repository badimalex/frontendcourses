    "babel-register": "^6.26.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "jest": "^23.5.0",
    "nyc": "^12.0.2",
    "react-addons-test-utils": "^15.6.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-config": "^1.0.8",
    "@types/react-router-dom": "^4.3.0",
    "antd": "^3.8.0",
    "contentful": "^7.0.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
