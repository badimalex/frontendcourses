    "react-router-dom": "^4.3.1",
    "typescript": "^3.0.1"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.test.ts",
      "src/**/*.test.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "all": true
  }
